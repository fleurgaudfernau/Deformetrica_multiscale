<?xml version="1.0"?>
<model>
    <model-type>Registration</model-type>
    <dimension>2</dimension>
    <dtype>float64</dtype>

    <template>
        <object id="pointcloud">
            <deformable-object-type>Landmark</deformable-object-type>
            <noise-std>0.1</noise-std>
            <filename>data/source_points.vtk</filename>
        </object>
    </template>
    <deformation-parameters>
        <kernel-width>3</kernel-width>
        <kernel-type>torch</kernel-type>
    </deformation-parameters>
</model>
