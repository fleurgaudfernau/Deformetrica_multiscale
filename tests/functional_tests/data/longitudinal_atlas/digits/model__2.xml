<?xml version="1.0" ?>
<model>
	    
	<model-type>LongitudinalAtlas</model-type>
	<dimension>2</dimension>
	<dtype>float64</dtype>
	<random-seed>42</random-seed>
	    
	<template>
		<object id="img">
			<deformable-object-type>Image</deformable-object-type>
			<filename>data/ForInitialization__Template_img.png</filename>
			<noise-std>0.5</noise-std>
		</object>
	</template>
	    
	<deformation-parameters>
		<concentration-of-timepoints>5</concentration-of-timepoints>
		<kernel-type>torch</kernel-type>
		<kernel-width>8</kernel-width>
		<number-of-sources>1</number-of-sources>
		<t0>10.0</t0>
	</deformation-parameters>

	<initial-control-points>data/ForInitialization__ControlPoints.txt</initial-control-points>
	<initial-momenta>data/ForInitialization__Momenta.txt</initial-momenta>
	<initial-onset-ages>data/ForInitialization__OnsetAges.txt</initial-onset-ages>
	<initial-log-accelerations>data/ForInitialization__LogAccelerations.txt</initial-log-accelerations>
	<initial-modulation-matrix>data/ForInitialization__ModulationMatrix.txt</initial-modulation-matrix>
	<initial-sources>data/ForInitialization__Sources.txt</initial-sources>
	<initial-time-shift-std>0.2</initial-time-shift-std>
	<initial-log-acceleration-std>0.8</initial-log-acceleration-std>
</model>

